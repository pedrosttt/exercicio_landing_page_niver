 &__logo {\r\n        margin: 0 auto;\r\n        height: 80px;\r\n        display: block;\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        padding: 40px 0;\r\n\r\n        &__logo {\r\n            height: 40px;\r\n        }\r\n\r\n        &__text {\r\n            font-size: 22px;\r\n            line-height: 24px;\r\n            margin: 24px 0;\r\n        }\r\n    }\r\n}\r\n","styles\\_variables.scss":"$textColor: #000; \r\n$secondaryTextColor: #fff; \r\n\r\n$purpleButtonColor: #ffcc00; \r\n\r\n$blue: #00f;\r\n$purple: #800080; \r\n$pink: #ff1493; \r\n$yellow: #ffd700; \r\n","styles\\components\\_infos_bar.scss":"@use '../variables';\r\n\r\n.infos-bar {\r\n    padding: 16px 24px;\r\n    background-color: variables.$textColor;\r\n    display: flex;\r\n    justify-content: space-between;\r\n\r\n    &__infos {\r\n        display: flex;\r\n        gap: 40px;\r\n\r\n        &__item {\r\n            font-size: 14px;\r\n            color: variables.$secondaryTextColor;\r\n\r\n            b {\r\n                color: variables.$secondaryTextColor;\r\n                font-size: 18px;\r\n                display: block;\r\n            }\r\n        }\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        display: block;\r\n\r\n        &__infos {\r\n            display: block;\r\n\r\n            &__item {\r\n                margin-bottom: 24px;\r\n            }\r\n        }\r\n    }\r\n\r\n    @media (max-width: 1024px) {\r\n        &__infos {\r\n            gap: 16px;\r\n        }\r\n    }\r\n}","styles\\components\\_buttons.scss":"@use '../variables';\r\n\r\n.button {\r\n    padding: 12px;\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    text-align: center;\r\n    white-space: nowrap;\r\n\r\n    &--primary {\r\n        color: variables.$textColor;\r\n        background-color: variables.$purpleButtonColor;\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        display: flex;\r\n        width: 100%;\r\n        align-items: center;\r\n    }\r\n\r\n    @media (max-width: 1024px) {\r\n        display: flex;\r\n        align-items: center;\r\n    }\r\n}\r\n\r\n","styles\\_event.scss":"@use 'variables';\r\n\r\n.event {\r\n    padding: 80px 0;\r\n\r\n    &--tematica {\r\n        background-image: linear-gradient(#87CEEB, #1E90FF); // Altere para a cor desejada\r\n    }\r\n\r\n    &--musica {\r\n        background-image: linear-gradient(#1E90FF, #228B22); // Altere para a cor desejada\r\n    }\r\n\r\n    &--comida-bebida {\r\n        background-image: linear-gradient(#228B22, #ADFF2F); // Altere para a cor desejada\r\n    }\r\n\r\n    &--surpresas {\r\n        background-image: linear-gradient(#ADFF2F, #87CEEB); // Altere para a cor desejada\r\n    }\r\n\r\n    &--image-left {\r\n        .container {\r\n            flex-direction: row-reverse;\r\n        }\r\n    }\r\n\r\n    .container {\r\n        display: flex;\r\n        gap: 40px;\r\n    }\r\n\r\n    &__image {\r\n        max-width: 360px;\r\n    }\r\n\r\n    &__details {\r\n        &__title {\r\n            font-size: 18px;\r\n            font-weight: bold;\r\n            margin-bottom: 16px;\r\n            color: variables.$textColor; // Adicione a cor do texto\r\n        }\r\n\r\n        &__description {\r\n            font-size: 16px;\r\n            line-height: 22px;\r\n            color: variables.$textColor; // Adicione a cor do texto\r\n        }\r\n    }\r\n\r\n    @media (max-width: 640px) {\r\n        .container {\r\n            display: block;\r\n        }\r\n\r\n        &__image {\r\n            width: 100%;\r\n        }\r\n\r\n        &__details {\r\n            margin-right: 0;\r\n            margin-bottom: 16px;\r\n        }\r\n    }\r\n}\r\n","styles\\main.scss":"@use 'hero';\r\n@use 'components/infos_bar';\r\n@use 'components/buttons';\r\n@use 'event';\r\n@use 'variables';\r\n\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: 'Roboto', sans-serif;\r\n    color: variables.$textColor;\r\n    list-style: none;\r\n}\r\n\r\n.container {\r\n    max-width: 960px;\r\n    width: 100%;\r\n    margin: 0 auto;\r\n\r\n    @media (max-width: 640px) {\r\n        max-width: 80%;\r\n    }\r\n\r\n    @media (max-width: 1024px) {\r\n        max-width: 90%;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"22b364624c01833e799a2dfe549493e3","cacheData":{"env":{}}}